<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="578.0" prefWidth="671.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.StatusReportViewController">
   <Text fx:id="statusReportTitle" style="-fx-font-size: 18px; -fx-font-weight: bold;" />
   <BarChart fx:id="statusReportChart" prefHeight="400.0" prefWidth="659.0">
      <xAxis>
         <CategoryAxis label="Day of the Month" />
      </xAxis>
      <yAxis>
         <NumberAxis label="Number of Subscribers" />
      </yAxis>
   </BarChart>
   <Text fx:id="chartOverlayText" strokeType="OUTSIDE" strokeWidth="0.0" text="Summary" />
   <Button onAction="#closeWindow" text="Close" />
</VBox>
